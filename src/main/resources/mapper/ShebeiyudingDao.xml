<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.ShebeiyudingDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.ShebeiyudingEntity" id="shebeiyudingMap">
        <result property="shebeibianhao" column="shebeibianhao"/>
        <result property="shebeimingcheng" column="shebeimingcheng"/>
        <result property="shebeileixing" column="shebeileixing"/>
        <result property="shebeitupian" column="shebeitupian"/>
        <result property="shebeishuliang" column="shebeishuliang"/>
        <result property="shebeixinghao" column="shebeixinghao"/>
        <result property="yudingshijian" column="yudingshijian"/>
        <result property="yudingbeizhu" column="yudingbeizhu"/>
        <result property="yudingrenyuan" column="yudingrenyuan"/>
        <result property="userid" column="userid"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.ShebeiyudingVO" >
		SELECT * FROM shebeiyuding  shebeiyuding         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.ShebeiyudingVO" >
		SELECT  shebeiyuding.* FROM shebeiyuding  shebeiyuding 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.ShebeiyudingView" >

		SELECT  shebeiyuding.* FROM shebeiyuding  shebeiyuding 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.ShebeiyudingView" >
		SELECT * FROM shebeiyuding  shebeiyuding <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
