<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.MafangDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.MafangEntity" id="mafangMap">
        <result property="mafangzhanghao" column="mafangzhanghao"/>
        <result property="mafangxingming" column="mafangxingming"/>
        <result property="mima" column="mima"/>
        <result property="shoujihao" column="shoujihao"/>
        <result property="xingbie" column="xingbie"/>
        <result property="nianling" column="nianling"/>
        <result property="touxiang" column="touxiang"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.MafangVO" >
		SELECT * FROM mafang  mafang         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.MafangVO" >
		SELECT  mafang.* FROM mafang  mafang 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.MafangView" >

		SELECT  mafang.* FROM mafang  mafang 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.MafangView" >
		SELECT * FROM mafang  mafang <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
